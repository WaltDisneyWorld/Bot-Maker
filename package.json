{
  "name": "discord-bot-creator",
  "description": "A free open-source tool to create Discord Bots.",
  "version": "1.0.0-alpha",
  "main": "main.js",
  "homepage": "https://discordbotcreator.github.io",
  "license": "GPL-3.0",
  "author": {
    "name": "Cap",
    "email": "cap.contact@aol.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/discord-bot-creator/discord-bot-creator.git"
  },
  "bugs": {
    "url": "https://github.com/discord-bot-creator/discord-bot-creator/issues"
  },
  "scripts": {
    "dev:react": "node scripts/reactDevRequirements && react-scripts start",
    "dev:electron": "electron ./main.js",
    "format": "prettier --write ./**/*.{js,ts,tsx} && eslint --fix ./**/*.{js,ts,tsx}",
    "build": "node scripts/reactBuild && electron-builder --win --x64",
    "build:react": "node scripts/reactBuild",
    "build:win": "electron-builder --win --x64"
  },
  "dependencies": {
    "@discord-bot-creator/bot": "1.0.7",
    "@discord-bot-creator/icons": "1.0.5",
    "decompress": "4.2.1",
    "electron-store": "6.0.1",
    "fs-extra": "9.0.1",
    "serve-handler": "6.1.3"
  },
  "devDependencies": {
    "@node-minify/core": "6.1.0",
    "@node-minify/cssnano": "6.1.0",
    "@node-minify/html-minifier": "6.1.0",
    "@node-minify/uglify-es": "6.1.0",
    "@types/node": "14.11.8",
    "@types/react": "16.9.52",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "5.1.4",
    "electron": "10.1.3",
    "electron-builder": "22.8.1",
    "glob": "7.1.6",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "sortablejs": "1.12.0",
    "styled-components": "5.2.0",
    "typescript": "4.0.3"
  }
